all:
	tar xvf autograde.tar
	cp sort.c C-sortlab-handout/src/
	cp ../src/src/main.c C-sortlab-handout/src/
	cp ../src/src/tests.c C-sortlab-handout/src/
	cp ../src/inc/tests.h C-sortlab-handout/inc/
	cp ../src/Makefile C-sortlab-handout/
	(cd C-sortlab-handout; make test)

clean:
	rm -rf *~ C-sortlab-handout